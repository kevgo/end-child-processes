{
  "name": "end-child-processes",
  "version": "1.0.3",
  "description": "Reliably ends all direct and indirect child processes of the current process",
  "homepage": "https://github.com/kevgo/end-child-processes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevgo/end-child-processes.git"
  },
  "license": "ISC",
  "author": "Kevin Goslar",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "coverage": "",
    "doc": "text-runner",
    "fix": "prettier --write . && eslint --fix --ext=.ts . && sort-package-json",
    "lint": "prettier -l . && eslint --ext=.ts . && sort-package-json --check",
    "setup": "npm install",
    "test": " npm run build && npm run fix && npm run lint && npm run unit",
    "unit": "mocha src/test.ts",
