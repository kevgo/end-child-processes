    "@types/node": "15.12.4",
    "@types/ps-tree": "1.1.0",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "coveralls": "3.1.0",
    "eslint": "7.29.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-typescript-sort-keys": "1.7.0",
    "mocha": "9.0.1",
    "nyc": "15.1.0",
    "prettier": "2.3.1",
    "sort-package-json": "1.57.0",
    "source-map-support": "0.5.19",
    "text-runner": "5.0.0",
    "textrun-make": "0.1.0",
    "ts-node": "10.0.0",
    "typescript": "4.3.4"
