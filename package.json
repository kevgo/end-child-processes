{
  "name": "end-child-processes",
  "version": "1.0.3",
  "description": "Reliably ends all direct and indirect child processes of the current process",
  "homepage": "https://github.com/kevgo/end-child-processes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevgo/end-child-processes.git"
  },
  "license": "ISC",
  "author": "Kevin Goslar",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "coverage": "",
    "doc": "text-runner",
    "fix": "dprint fmt && eslint --fix --ext=.ts . && sort-package-json",
    "lint": "dprint check && eslint --ext=.ts . && sort-package-json --check",
    "setup": "npm install",
    "test": " npm run build && npm run fix && npm run lint && npm run unit",
    "unit": "mocha src/test.ts",
    "update": "npm-check-updates -u && npm install"
  },
  "dependencies": {
    "debug": "4.3.1",
    "ps-tree": "1.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@types/debug": "4.1.5",
    "@types/mocha": "8.2.2",
    "@types/node": "15.12.4",
    "@types/ps-tree": "1.1.0",
    "dprint": "0.49.0",
    "eslint": "9.22.0",
    "mocha": "9.0.1",
    "sort-package-json": "1.57.0",
    "text-runner": "5.0.0",
    "textrun-make": "0.1.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1"
  }
}
