{
  "name": "end-child-processes",
  "version": "1.0.3",
  "description": "Reliably ends all direct and indirect child processes of the current process",
  "homepage": "https://github.com/kevgo/end-child-processes#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kevgo/end-child-processes.git"
  },
  "license": "ISC",
  "author": "Kevin Goslar",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/src/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "coverage": "node --test --import tsx --experimental-test-coverage --test-reporter=lcov --test-reporter-destination=lcov.info -- src/*.test.ts",
    "doc": "text-runner",
    "fix": "dprint fmt && eslint --fix --ext=.ts . && sort-package-json --quiet",
    "lint": "dprint check && eslint --ext=.ts . && sort-package-json --check --quiet",
    "setup": "npm install",
    "test": " npm run build && npm run fix && npm run lint && npm run unit",
    "unit": "npm run build && node --test dist/test/test.js",
    "update": "npm-check-updates -u && npm install"
  },
  "dependencies": {
    "@fengmk2/ps-tree": "^2.0.2",
    "debug": "4.4.0"
  },
  "devDependencies": {
    "@eslint/js": "9.22.0",
    "@types/debug": "4.1.12",
    "@types/node": "22.13.10",
    "@types/ps-tree": "1.1.6",
    "dprint": "0.49.0",
    "eslint": "9.22.0",
    "npm-check-updates": "17.1.15",
    "sort-package-json": "3.0.0",
    "text-runner": "7.1.0",
    "textrun-make": "0.3.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "typescript-eslint": "8.26.1"
  }
}
